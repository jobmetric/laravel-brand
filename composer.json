{
    "name": "jobmetric/laravel-brand",
    "description": "This is a brand management package for Laravel that you can use in your projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "brand",
        "management"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-brand",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.12",
        "jobmetric/laravel-translation": "^2.6",
        "jobmetric/laravel-metadata": "^2.2",
        "jobmetric/laravel-membership": "^1.2",
        "jobmetric/laravel-layout": "^1.1",
        "jobmetric/laravel-url": "^1.0",
        "jobmetric/media": "^1.0",
        "spatie/laravel-query-builder": "^5.7"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Brand\\": "src/",
            "JobMetric\\Brand\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JobMetric\\Brand\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Brand\\BrandServiceProvider"
            ],
            "aliases": {
                "Brand": "JobMetric\\Brand\\Facades\\Brand"
            }
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-brand/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-brand"
    }
}
